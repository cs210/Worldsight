{
  "name": "todo",
  "version": "1.0.0",
  "description": "mern tutorial todo app",
  "main": "index.js",
  "scripts": {
    "start": "nodemon bootstrap.js",
    "start-watch": "nodemon node.js",
    "dev": "concurrently \"yarn run start-watch\" \"cd client && yarn start\"",
    "build-client": "cd client && yarn build",
    "prod": "yarn run postinstall && NODE_ENV=production yarn run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "sh -c \"yarn --silent are-you-es5 check -r -a client | tail -n 1 > ./non_ES5_node_modules \""
  },
  "author": "Jihee Hwang",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/helper-module-imports": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "aws-sdk": "^2.658.0",
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet-csp": "^2.10.0",
    "ignore-styles": "^5.0.1",
    "material-ui": "^0.20.2",
    "mongoose": "^5.9.9",
    "prop-types": "^15.7.2",
    "props": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone-s3-uploader": "^1.1.0",
    "react-router": "^5.1.2",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "underscore.string": "^3.3.5",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "are-you-es5": "^1.3.3",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "nodemon": "^2.0.3"
  }
}
